#include <iostream>
using namespace std;

int main(int argc, char* argv[])
{
    cout << 1 + rand() % 6 + 1 << endl;
    cout << 1 + rand() % 6 + 1 << endl;
    //system ("pause");
}
2
#include <iostream>

using namespace std;

int main()
{
    int n,a0,a1,a2;
    cin>>n;
    a0=0;
    a1=1;
    a2=1;
    if(n==0){
        cout<<0;
    }
    else if(n==1){
        cout<<1;
    }
    else{
        for(int i=0;i<n-1;i++){
            a2=a0+a1;
            a0=a1;
            a1=a2;
    }
    cout<<a2;
    }
}
3
#include <iostream>
using namespace std;

int main()
{
    int Sum, tmpSum;
    for (int n = 1; n <= 10000; n++){
        Sum = 0;
        tmpSum = 0;
        for (int i = 1; i < n;i++){
            if (n%i == 0){
                Sum = Sum + i;
            }
        }
        for (int i = 1; i < Sum; i++){
            if (Sum%i == 0){
                tmpSum += i;
            }
        }
        if (n == tmpSum){
            cout << n << '-' << Sum << endl;
        }
    }
}
3/1 (нахождение суммы делителей)
#include <iostream>
using namespace std;

int main()
{
    int n, sum = 0;
    cin>>n;
    for (int i = 1; i <n; i++){
        sum += i * (n % i == 0);
    }
    cout << sum<<" ";
    return 0;
}
